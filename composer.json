{
  "name": "fusio/adapter-file",
  "description": "Adapter to serve static files",
  "homepage": "https://www.fusio-project.org",
  "keywords": ["fusio-adapter"],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "https://chrisk.app"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-fileinfo": "*",
    "fusio/engine": "^6.0",
    "league/flysystem": "^3.0",
    "symfony/yaml": "^5.0|^6.0",
    "ramsey/uuid": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "vimeo/psalm": "^5.9"
  },
  "autoload": {
    "psr-4": {
      "Fusio\\Adapter\\File\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fusio\\Adapter\\File\\Tests\\": "tests/"
    }
  }
}
